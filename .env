
DOCKER_REPO=store/thingsboard

JS_EXECUTOR_DOCKER_NAME=tb-pe-js-executor

TB_VERSION=2.4.2.1PE

# Database used by ThingsBoard, can be either postgres (PostgreSQL) or cassandra (Cassandra).
